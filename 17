#include <stdio.h>

int main() {
    int vetor[10];

    printf("Digite 10 n√∫meros:\n");
    for (int i = 0; i < 10; ++i) {
        scanf("%d", &vetor[i]);
    }

    printf("Vetor original:\n");
    for (int i = 0; i < 10; ++i) {
        printf("%d ", vetor[i]);
    }
    printf("\n");

    printf("Modificando valores negativos para 0...\n");
    for (int i = 0; i < 10; ++i) {
        if (vetor[i] < 0) {
            vetor[i] = 0;
        }
    }

    printf("Vetor modificado:\n");
    for (int i = 0; i < 10; ++i) {
        printf("%d ", vetor[i]);
    }
    printf("\n");

    return 0;
}
