#include <stdio.h>

int main() {
    float vetor[50];

    for (int i = 0; i < 50; ++i) {
        vetor[i] = (i + 5) * (i + 1); // Aplicando a fórmula (i + 5) * (i + 1)
    }

    printf("Vetor preenchido com a fórmula (i + 5) * (i + 1):\n");
    for (int i = 0; i < 50; ++i) {
        printf("%.2f ", vetor[i]);
    }
    printf("\n");

    return 0;
}
